            <HBox AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="10.0">
               <children>
                  <TextField fx:id="amountTxF" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="55.0" />
                      <VBox>
                          <children>
                              <Label fx:id="cardCategory" styleClass="tagText" text="LABEL" textFill="#9e9e9e">
                                  <font>
                                      <Font name="SF Pro Display Regular" size="9.0" />
