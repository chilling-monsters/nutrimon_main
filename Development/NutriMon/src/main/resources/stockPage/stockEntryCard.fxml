<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<fx:root maxWidth="-Infinity" minWidth="-Infinity" prefWidth="300.0" type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>

      <AnchorPane prefHeight="60.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
         <children>
            <AnchorPane AnchorPane.bottomAnchor="10.0">
               <children>
                  <HBox AnchorPane.leftAnchor="15.0">
                     <children>
                        <ImageView fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../img/ic_duration2x.png" />
                           </image>
                        </ImageView>
                        <Label fx:id="entryAddedDate" styleClass="detailText" text="Label" textFill="#9e9e9e">
                           <font>
                              <Font name="SF Pro Text Regular" size="12.0" />
                           </font>
                           <HBox.margin>
                              <Insets left="10.0" />
                           </HBox.margin>
                        </Label>
                     </children>
                  </HBox>
                  <HBox AnchorPane.leftAnchor="150.0">
                     <children>
                        <ImageView fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../img/ic_quantity2x.png" />
                           </image>
                        </ImageView>
                        <Label fx:id="entryAmount" styleClass="detailText" text="Label" textFill="#9e9e9e">
                           <font>
                              <Font name="SF Pro Text Regular" size="12.0" />
                           </font>
                           <HBox.margin>
                              <Insets left="10.0" />
                           </HBox.margin>
                        </Label>
                     </children>
                  </HBox>
               </children>
            </AnchorPane>
            <Label fx:id="entryTimeLeft" styleClass="headerText" text="Label" AnchorPane.leftAnchor="15.0" AnchorPane.topAnchor="10.0">
               <font>
                  <Font name="SF Pro Display Regular" size="15.0" />
               </font>
            </Label>
         </children>
         <styleClass>
            <String fx:value="entryCardPane" />
            <String fx:value="card" />
            <String fx:value="cardPane" />
            <String fx:value="pane" />
         </styleClass>
      </AnchorPane>

   </children>
   <effect>
      <DropShadow blurType="ONE_PASS_BOX" color="#00000080" height="50.0" offsetX="5.0" offsetY="5.0" radius="24.5" width="50.0" />
   </effect>
</fx:root>
